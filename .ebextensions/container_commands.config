container_commands:
  01-bundle_install:
    command: bundle install --path vendor/bundle
  02-db_migrate:
    command: bundle exec rake db:migrate
  03-assets_precompile:
    command: bundle exec rake assets:precompile
  04-chmod_log:
    command: "sudo chmod 666 /var/app/current/log/cron.log /var/app/current/log/production.log"