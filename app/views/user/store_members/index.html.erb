<h1>会員一覧</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">名前</th>
      <th scope="col">電話番号</th>
      <th scope="col">email</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% @store_members.each do |member| %>
      <tr>
        <th scope="row"><%= member.name %></th>
        <td><%= member.phone %></td>
        <td><%= member.email %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>