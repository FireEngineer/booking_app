<h1>認証</h1>
<%= form_with url:google_auth_identifier_path, method: :patch do |f| %>
  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">client_id：</label>
    <div class="col-sm-10">
      <%= f.text_field :client_id, class:"form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword" class="col-sm-2 col-form-label">client_secret：</label>
    <div class="col-sm-10">
      <%= f.text_field :client_secret, class:"form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword" class="col-sm-2 col-form-label">calendar_id：</label>
    <div class="col-sm-10">
      <%= f.text_field :google_calendar_id, class:"form-control" %>
    </div>
  </div>

  <%= f.submit "認証へ", class:"btn btn-primary" %>
<% end %>